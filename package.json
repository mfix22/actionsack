{
  "name": "actionsack",
  "version": "0.0.14",
  "description": "React UX components for handling common interactions",
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest",
    "coverage": "yarn test --coverage",
    "build": "microbundle --jsx React.createElement",
    "dev": "microbundle --jsx React.createElement watch",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "eslint src",
    "prettier": "prettier --single-quote --no-semi --print-width 100 --write src/*.js test/*.js",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "react",
    "ux",
    "hooks",
    "interactions",
    "collection"
  ],
  "author": "mfix22",
  "license": "MIT",
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "react-click-outside": "^3.0.1",
    "react-fast-compare": "3.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/react": "^11.2.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "docz": "^2.3.1",
    "docz-theme-default": "^1.2.0",
    "docz-utils": "^2.3.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "microbundle": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "jest": {
    "clearMocks": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfix22/actionsack.git"
  },
  "bugs": {
    "url": "https://github.com/mfix22/actionsack/issues"
  }
}
