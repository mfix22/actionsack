{
  "name": "actionsack",
  "version": "0.0.13",
  "description": "React UX components for handling common interactions",
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest",
    "coverage": "yarn test --coverage",
    "build": "microbundle --jsx React.createElement",
    "dev": "microbundle --jsx React.createElement watch",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "lint": "eslint src",
    "prettier": "prettier --single-quote --no-semi --print-width 100 --write src/*.js test/*.js",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "react",
    "ux",
    "hooks",
    "interactions",
    "collection"
  ],
  "author": "mfix22",
  "license": "MIT",
  "dependencies": {
    "copy-to-clipboard": "^3.3.1",
    "react-click-outside": "^3.0.1",
    "react-fast-compare": "2.0.4"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "docz": "^1.3.2",
    "docz-theme-default": "^1.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "microbundle": "^0.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "clearMocks": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfix22/actionsack.git"
  },
  "bugs": {
    "url": "https://github.com/mfix22/actionsack/issues"
  }
}
